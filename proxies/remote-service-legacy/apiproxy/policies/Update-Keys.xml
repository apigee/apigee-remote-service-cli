<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations async="false" continueOnError="false" enabled="true" name="Update-Keys" mapIdentifier="remote-service">
    <DisplayName>Update Keys</DisplayName>
    <Properties/>
    <ExclusiveCache>false</ExclusiveCache>
    <ExpiryTimeInSecs>2</ExpiryTimeInSecs>
    <Put override="true">
        <Key>
            <Parameter>kid</Parameter>
        </Key>
        <Value ref="kid"/>
    </Put>
    <Put override="true">
        <Key>
            <Parameter>jwks</Parameter>
        </Key>
        <Value ref="jwks"/>
    </Put>
    <Put override="true">
        <Key>
            <Parameter>private_key</Parameter>
        </Key>
        <Value ref="private_key"/>
    </Put>
    <Scope>environment</Scope>
</KeyValueMapOperations>