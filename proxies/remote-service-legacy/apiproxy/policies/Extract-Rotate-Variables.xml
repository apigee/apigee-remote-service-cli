<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="false" enabled="true" name="Extract-Rotate-Variables">
    <DisplayName>Extract Rotate Variables</DisplayName>
    <Properties/>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <JSONPayload>
        <Variable name="kid" type="string">
            <JSONPath>$.kid</JSONPath>
        </Variable>
        <Variable name="jwks" type="string">
            <JSONPath>$.jwks</JSONPath>
        </Variable>
        <Variable name="private_key" type="string">
            <JSONPath>$.private_key</JSONPath>
        </Variable>
    </JSONPayload>
    <Source clearPayload="true">request</Source>
</ExtractVariables>